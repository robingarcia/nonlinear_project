%% Negative autoregulation
% Negative autoregulation (NAR) is one of the simplest network motifs in
% E.coli. In NAR a transcription factor (TF) represses its own
% transcription. 

% Model class for NAR


% Definition of Parameters
syms n;
syms K = 0.1;
syms a = 1;
syms b = 1;
syms X;


syms n = 1;
syms K = 0.1;
syms a = 1;
syms b = 1;

% ODE NAR
X = sym('X');
t = sym('t');
dX/dt = b*(1/(1+(X/K)^n))-a*X;
